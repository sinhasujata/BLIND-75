   "source": [
    "# 2 pointers\n",
    "\n",
    "left, right = 0, 1\n",
    "maxP = 0\n",
    "\n",
    "while right < len(prices):\n",
    "    if prices[left] < prices[right]:\n",
    "        profit = prices[right] - prices[left]\n",
    "        maxP = max(profit, maxP)\n",
    "    else:\n",
    "        left = right\n",
    "    right += 1\n",
    "\n",
    "return maxP"
   ]
  }
