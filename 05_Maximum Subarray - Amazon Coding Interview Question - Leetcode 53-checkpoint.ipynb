   "source": [
    "# Sliding window O(n)\n",
    "        maxSum = nums[0]\n",
    "        currSum = 0\n",
    "\n",
    "        for n in nums:\n",
    "            if currSum < 0:\n",
    "                currSum = 0\n",
    "            currSum += n\n",
    "            maxSum = max(currSum, maxSum)\n",
    "        return maxSum"
   ]
  }
