   "source": [
    "# using prefix and postfix\n",
    "\n",
    "    result = [1]*len(nums)\n",
    "\n",
    "    prefix = 1\n",
    "    for i in range(nums):\n",
    "        result[i] = prefix\n",
    "        prefix *= nums[i]\n",
    "    postfix = 1\n",
    "\n",
    "    for i in range(len(nums)-1, -1, -1):\n",
    "        result[i] *= postfix\n",
    "        postfix *= nums[i]\n",
    "\n",
    "    return result"
   ]
  }
 ],
