   "source": [
    "# O(n^2)\n",
    "        LIS = [1]*len(nums)\n",
    "        \n",
    "        for i in range(len(nums)-1, -1, -1):\n",
    "            for j in range(i+1, len(nums)):\n",
    "                if nums[i] < nums[j]:\n",
    "                    LIS[i] = max(LIS[i], 1 + LIS[j])\n",
    "                    \n",
    "        return max(LIS)"
   ]
  }
